package com.question;

public class Sample {

	public static void main(String[] args) {
		String s="()()()((((())()()()()()";
		int count=0;
		for(int i=0;i<s.length()-1;) {
			char c=s.charAt(i);
			char d=s.charAt(i+1);
			if(c=='(' && d==')') {
				count++;
				i=i+1;
			}
		}
		System.out.println("prin >> "+count);
	}
}
