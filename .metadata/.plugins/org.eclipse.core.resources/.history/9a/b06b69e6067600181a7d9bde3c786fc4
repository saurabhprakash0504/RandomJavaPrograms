package com.question;

public class Sample {

	public static void main(String[] args) {
		String s="()()()((((())()()()()()";
		int count=0;
		int big=0;
		for(int i=0;i<s.length()-1;) {
			char c=s.charAt(i);
			char d=s.charAt(i+1);
			if(c=='(' && d==')') {
				
				count++;
				if(count>big) {
					big=count;
					count=0;
				}
				
				i=i+2;
			}
			else {
				i=i+1;
			}
		}
		System.out.println("prin >> "+count);
	}
}
