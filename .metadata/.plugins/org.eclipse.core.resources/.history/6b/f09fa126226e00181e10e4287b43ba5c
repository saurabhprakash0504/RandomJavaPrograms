	public static void main(String[] args) {
		List<? extends Number> l=new ArrayList<>();
		l.add(new Integer(3));
		l.add(5);
		
		System.out.println("l >> "+l);
	}
	
	void method(List<? extends Number> a) {
		
	}
	
}
